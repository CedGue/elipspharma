<assembly>
	<id>${project.version}-project</id>
	<formats>
		<format>zip</format>
	</formats>
	
	<includeBaseDirectory>true</includeBaseDirectory>
	
	<fileSets>
		<fileSet>
			<directory></directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>**/*</include>
			</includes>
			<excludes>
				<exclude>**/target/**</exclude>
			</excludes>
		</fileSet>
	</fileSets>
	
</assembly>
